<apex:page standardController="Account" recordSetVar="accounts" >
    <apex:pageBlock title="My Accounts">
        <apex:pageBlockTable value="{!account}" var="item">
            <apex:column value="{!item.name}"/>
            <apex:column value="{!item.CreatedDAte}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>